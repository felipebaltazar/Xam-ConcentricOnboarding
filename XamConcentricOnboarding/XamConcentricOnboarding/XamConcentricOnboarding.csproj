<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
    <AssemblyName>XamConcentricOnboarding</AssemblyName>
    <RootNamespace>XamConcentricOnboarding</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\background.png" />
    <None Remove="Images\skia.png" />
    <None Remove="Images\xamarin.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Images\background.png" />
    <EmbeddedResource Include="Images\skia.png" />
    <EmbeddedResource Include="Images\xamarin.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AsyncAwaitBestPractices" Version="3.1.0" />
    <PackageReference Include="SkiaSharp.Views.Forms" Version="1.68.0" />
    <PackageReference Include="Xamarin.Forms" Version="4.3.0.947036" />
    <PackageReference Include="Xamarin.Essentials" Version="1.2.0" />
    <PackageReference Include="Xamarin.HotReload" Version="1.5.3" />
  </ItemGroup>
</Project>